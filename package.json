{
  "name": "Popup-my-Bookmarks",
  "version": "6.0.0",
  "engines": {
    "node": ">=8.9"
  },
  "browserslist": ["chrome >= 51"],
  "description": "A more efficient way to view and manage your bookmarks menu.",
  "private": true,
  "license": "MIT",
  "author": "foray1010",
  "homepage": "https://github.com/foray1010/Popup-my-Bookmarks",
  "repository": "foray1010/Popup-my-Bookmarks",
  "bugs": "https://github.com/foray1010/Popup-my-Bookmarks/issues",
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack",
    "dev": "cross-env NODE_ENV=development webpack",
    "lint": "npm-run-all -p lint:*",
    "lint:css": "stylelint src/**/*.css src/**/*.js",
    "lint:flow": "flow check",
    "lint:js": "eslint .",
    "lint-staged": "lint-staged",
    "locales": "node scripts/generateLocalesFromTransifex.js",
    "md": "node scripts/generateMarkdown.js",
    "precommit": "npm-run-all -p lint:flow lint-staged",
    "test": "cross-env NODE_ENV=test jest"
  },
  "importSort": {
    ".js": {
      "parser": "import-sort-parser-babylon",
      "style": "import-sort-style-module"
    }
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": ["<rootDir>/src/**/*.js"],
    "coveragePathIgnorePatterns": ["/__fixtures__/"],
    "coverageReporters": ["lcov", "text-summary"],
    "moduleNameMapper": {
      "\\.css$": "<rootDir>/__mocks__/styleMock.js",
      "\\.png$": "<rootDir>/__mocks__/fileMock.js"
    }
  },
  "lint-staged": {
    "*.css": ["prettier-eslint --write", "stylelint --fix", "git add"],
    "*.js": ["import-sort --write", "prettier-eslint --write", "eslint --fix", "git add"],
    "*.json": ["prettier-eslint --write", "git add"],
    "*ignore-sync": ["ignore-sync", "git add"]
  },
  "dependencies": {
    "babel-polyfill": "^6.5.0",
    "classnames": "^2.2.3",
    "emotion": "^9.0.1",
    "lodash.debounce": "^4.0.8",
    "open-color": "^1.4.0",
    "prop-types": "^15.5.4",
    "ramda": "^0.25.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-emotion": "^9.0.1",
    "react-event-listener": "^0.5.3",
    "react-helmet": "^5.2.0",
    "react-measure": "^2.0.0",
    "react-redux": "^5.0.0",
    "react-virtualized": "^9.5.0",
    "redux": "^3.3.1",
    "redux-saga": "^0.16.0",
    "reduxsauce": "^0.7.0",
    "remote-redux-devtools": "^0.5.12",
    "seamless-immutable": "^7.0.0",
    "store": "^2.0.12",
    "typeface-archivo-narrow": "0.0.54",
    "webextension-polyfill": "^0.2.1"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-jest": "^22.2.2",
    "babel-loader": "^7.0.0",
    "babel-plugin-emotion": "^9.0.1",
    "babel-preset-foray1010": "^7.4.0",
    "bluebird": "^3.5.1",
    "chance": "^1.0.13",
    "chrome-manifest-loader": "^0.1.1",
    "clean-webpack-plugin": "^0.1.15",
    "copy-webpack-plugin": "^4.1.1",
    "cross-env": "^5.0.0",
    "css-loader": "^0.28.0",
    "duplicate-package-checker-webpack-plugin": "^2.1.0",
    "eslint": "^4.8.0",
    "eslint-config-foray1010": "^6.2.0",
    "extract-loader": "^1.0.0",
    "extract-text-webpack-plugin": "^3.0.1",
    "file-loader": "^1.1.5",
    "flow-bin": "^0.66.0",
    "fs-extra": "^5.0.0",
    "html-webpack-plugin": "^3.0.4",
    "husky": "^0.14.0",
    "ignore-sync": "^1.0.0",
    "image-process-loader": "^0.1.0",
    "import-sort-cli": "^4.2.0",
    "import-sort-parser-babylon": "^4.2.0",
    "import-sort-style-module": "^4.2.0",
    "jest": "^22.3.0",
    "lint-staged": "^7.0.0",
    "npm-run-all": "^4.1.1",
    "postcss-cssnext": "^3.0.0",
    "postcss-import": "^11.0.0",
    "postcss-loader": "^2.0.7",
    "postcss-normalize": "^4.0.0",
    "postcss-url": "^7.0.0",
    "prettier-eslint-cli": "^4.3.2",
    "prompt": "^1.0.0",
    "script-ext-html-webpack-plugin": "^1.7.1",
    "sharp": "^0.20.0",
    "style-loader": "^0.20.1",
    "stylelint": "^9.0.0",
    "stylelint-config-foray1010": "^4.0.0",
    "stylelint-processor-styled-components": "^1.2.2",
    "transifex": "^1.4.4",
    "uglifyjs-webpack-plugin": "^1.0.1",
    "webpack": "^3.7.1",
    "webpack-bundle-analyzer": "^2.3.0",
    "yaml-loader": "^0.5.0",
    "zip-webpack-plugin": "^2.0.0"
  }
}
