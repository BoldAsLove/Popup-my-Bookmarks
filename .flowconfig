[options]
experimental.const_params=true
module.use_strict=true

# webpack loaders
module.name_mapper.extension='yml' -> '<PROJECT_ROOT>/__mocks__/fileMock.js'

[lints]
deprecated-call-syntax=off
nonstrict-import=error
sketchy-null=error
sketchy-number=error
unsafe-getters-setters=error
untyped-type-import=error

[strict]
deprecated-type
